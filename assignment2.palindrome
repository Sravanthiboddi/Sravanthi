/*A program to print given number is palindrome or not*/
#include <stdio.h>
int main()
{
   int n,sum=0,r,temp=0;
   puts("enter the number");
   scanf("%d",&n);
   temp=n;
   while(n>0)
   {
       r=n%10;
       sum=sum*10+r;
       n=n/10;
   }
  if(temp==sum)
   printf("palindrome");
  else
   printf("not palindrome");
    return 0;
}
