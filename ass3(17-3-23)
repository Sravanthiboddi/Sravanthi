//Write a c program to find n factorial using sub function
#include<stdio.h>
int fact(int n);
int main()
{
    int n,f;
    scanf("%d",&n);
    f=fact(n);
    printf("%d",f);
    return 0;
}
int fact(int x)
{
    int fac=1;
    for(int i=1;i<=x;i++)
     {
         fac=fac*i;
     }
     return fac;
}
