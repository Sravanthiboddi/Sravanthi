#include <stdio.h>

int main()
{
  int n,i,a[100];
   scanf("%d",&n);
   for (int i=1;i<=n;i++)
   {
       scanf("%d",&a[i]);
       
          if(a[i]<=6&&a[i]>=1)
            printf("Baby age");
          else if(a[i]>=6&&a[i]<=18)
            printf("School age");
          else 
            printf("Adult age");
   }

    return 0;
}
