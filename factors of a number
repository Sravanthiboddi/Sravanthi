// c program to print factors of a given number.
#include <stdio.h>
int main()
{
    int n;
    puts("enter the number");
    scanf("%d",&n);
    puts("the factors of a number are");
    for(int i=1;i<=n;i++)
     { 
        if(n%i==0)
          printf("%d ",i);
        else
        continue;
     }
     return 0;
}  
